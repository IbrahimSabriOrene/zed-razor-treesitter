{
  "`$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "razor",
      "camelcase": "Razor",
      "title": "Razor",
      "scope": "source.razor",
      "file-types": [
        "razor",
        "cshtml"
      ],
      "injection-regex": "^razor$",
      "class-name": "TreeSitterRazor"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Razor grammar for tree-sitter",
    "authors": [
      {
        "name": "ibrahim",
        "email": "ibrahimsabriorene@protonmail.com",
        "url": "www.b.co"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-razor",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": false,
    "java": false,
    "node": true,
    "python": false,
    "rust": false,
    "swift": false,
    "zig": false
  }
}